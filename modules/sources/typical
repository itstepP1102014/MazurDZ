void input(int array[], int size)
{
    for(int i = 0; i < size; ++i)
    {
        printf("Enter element[%d]:\n", i);
        scanf("%d", &array[i]);
    }
}

void output(const int array[], int size)
{
    for(int i = 0; i < size; ++i)
    {
        printf("%d ", array[i]);
    }
    printf("\n");
}
